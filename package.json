{
  "name": "daraz-label-printing",
  "productName": "Daraz Label Printing",
  "version": "1.0.1",
  "description": "This application automatically separates and combine order invoice and label from pdf to save time and printing cost",
  "private": true,
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "canvas": "^2.11.0",
    "electron-is-dev": "^2.0.0",
    "pdfjs-dist": "^3.4.120",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-router-dom": "^6.9.0",
    "react-scripts": "5.0.1",
    "smalltalk": "^4.1.1",
    "web-vitals": "^2.1.4"
  },
  "main": "public/main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:serve": "concurrently -k \"cross-env Browser=none npm start\" \"npm run electron:start\"",
    "electron:build": "npm run build && electron-builder --win --x64 -c.extraMetadata.main=build/main.js",
    "electron:start": "wait-on http://127.0.0.1:3000 && electron ."
  },
  "build": {
    "extends": null,
    "npmRebuild": false,
    "appId": "com.smjwsb.daraz-label-printing",
    "productName": "Daraz Label Printing",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "win": {
      "target": "portable",
      "icon": "public/icon.ico"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "keywords": [
    "daraz",
    "sellercenter",
    "daraz label",
    "shipping label"
  ],
  "author": "Syed Jamal",
  "browserslist": {
    "production": [
      "last 1 electron version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^23.1.3",
    "electron-builder": "^23.6.0",
    "electron-packager": "^17.1.1",
    "wait-on": "^7.0.1"
  }
}
